                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "integer",
                                            "example": 401
                                        },
                                        "message": {
                                            "type": "string",
                                            "enum": [
                                                "Unauthorized. Token has expired.",
                                                "Unauthorized. Token mismatch.",
                                                "Unauthorized. Token not provided.",
                                                "Unauthorized. Invalid token."
                                            ]
                                        }
                                    },
                                    "required": ["status", "message"]
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "summary": "Delete an existing Data",
                "tags": ["Master"],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "description": "ID of the api to delete",
                        "schema": {
                            "type": "string",
                            "example": "9d26046e-9ded-4b38-87b8-dc275e14d509"
                        }
                    }
                ],
                "responses": {
